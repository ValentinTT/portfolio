---
import { Image } from "astro:assets";
export interface Props {
  class?: string;
  w?: number | `${number}`;
  h?: number | `${number}`;
  src: string;
  alt?: string;
}

const { src, alt, class: className, w, h } = Astro.props;
---

<Image
  src={src}
  alt={alt ?? "Image Logo"}
  width={w ?? "50"}
  height={h ?? "50"}
  class:list={[
    "bg-transparent rounded-full border-2 border-zinc-900 overflow-hidden",
    className,
  ]}
/>
